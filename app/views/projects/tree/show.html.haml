- @no_container = true

- page_title @path.presence || "文件", @ref
= content_for :meta_tags do
  = auto_discovery_link_tag(:atom, namespace_project_commits_url(@project.namespace, @project, @ref, rss_url_options), title: "#{@project.name}:#{@ref} 提交")
= render "projects/commits/head"
= render 'projects/last_push'

%div{ class: container_class }
  = render 'projects/files'
